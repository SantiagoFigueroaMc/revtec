<p style="color: green"><%= notice %></p>

<h1>Clients</h1>

<div id="clients">
  <% @clients.each do |client| %>
    <div class="card">
      <%= render client %>
      <p>
        <%= link_to "Show this client", client %>
      </p>
    </div>
  <% end %>
</div>

<%= link_to "New client", new_client_path %>
